{
  "name": "@acorcutt/firestarter",
  "version": "0.0.18",
  "description": "React firebase components for getting up and running quickly.",
  "source": "lib/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "engines": {
    "node": ">= 14"
  },
  "files": [
    "/dist",
    "/lib"
  ],
  "scripts": {
    "dev": "next dev",
    "build:package": "rm -rf dist; tsup && tsup --dts-only; rm -rf pack; mkdir pack; pnpm pack --pack-destination pack",
    "build:next": "next build",
    "start": "next start",
    "lint": "next lint",
    "emulators": "firebase emulators:start"
  },
  "tsup": {
    "tsconfig": "./lib/tsconfig.json",
    "entry": [
      "lib/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ]
  },
  "keywords": [
    "react",
    "firebase",
    "nextjs"
  ],
  "author": "Anthony Corcutt",
  "license": "MIT",
  "homepage": "https://github.com/acorcutt/firestarter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acorcutt/firestarter.git"
  },
  "bugs": {
    "url": "https://github.com/acorcutt/firestarter/issues"
  },
  "dependencies": {
    "firebase": "^9.9.0",
    "next": "^12.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.33.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "@types/react": "^18.0.15",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.20.0",
    "eslint-config-next": "^12.2.2",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.6",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4",
    "clsx": "^1.2.1",
    "fast-deep-equal": "^3.1.3",
    "mitt": "^3.0.0",
    "nanoid": "^3.3.4",
    "store2": "^2.14.2"
  },
  "peerDependencies": {
    "firebase": "^9",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7"
  }
}
