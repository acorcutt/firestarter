{
  "name": "@acorcutt/firestarter",
  "version": "0.0.5",
  "description": "React firebase components for getting up and running quickly.",
  "source": "lib/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "engines": {
    "node": ">= 14"
  },
  "targets": {
    "default": {
      "includeNodeModules": [
        "clsx",
        "fast-deep-equal",
        "mitt",
        "store2"
      ]
    }
  },
  "scripts": {
    "parcel": "parcel build",
    "watch": "parcel watch",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "emulators": "firebase emulators:start"
  },
  "keywords": [
    "react",
    "firebase"
  ],
  "author": "Anthony Corcutt",
  "license": "MIT",
  "homepage": "https://github.com/acorcutt/firestarter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acorcutt/firestarter.git"
  },
  "bugs": {
    "url": "https://github.com/acorcutt/firestarter/issues"
  },
  "devDependencies": {
    "@parcel/core": ">=2.6.0 <3.0.0",
    "@parcel/packager-ts": "2.6.0",
    "@parcel/transformer-typescript-types": "2.6.0",
    "@types/node": "^17.0.40",
    "@types/react": "^18.0.12",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.17.0",
    "eslint-config-next": "^12.1.6",
    "parcel": "^2.6.0",
    "postcss": "^8.4.14",
    "process": "^0.11.10",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "fast-deep-equal": "^3.1.3",
    "firebase": "^9.8.2",
    "mitt": "^3.0.0",
    "next": "^12.1.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-hook-form": "^7.31.3",
    "store2": "^2.13.2"
  },
  "peerDependencies": {
    "firebase": "9",
    "next": "12",
    "react": "18",
    "react-dom": "18",
    "react-hook-form": "8"
  }
